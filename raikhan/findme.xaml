<Window x:Class="raikhan.findme"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:raikhan"
        mc:Ignorable="d"
        Title="findme" Height="300" Width="300">
    <Grid>
        <Button Content="Поиск" HorizontalAlignment="Left" Margin="203,121,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
        <Label Content="Баркод" HorizontalAlignment="Left" Margin="23,59,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.091,-0.123" Width="114" Height="47" FontSize="18" FontFamily="Rage Italic" FontWeight="Bold"/>
        <TextBox x:Name="find" HorizontalAlignment="Left" Height="23" Margin="148,67,0,0" TextWrapping="Wrap" Text="Введите баркод" VerticalAlignment="Top" Width="120"/>
        <DataGrid HorizontalAlignment="Left"  x:Name="dtGrid" ItemsSource="{Binding}" Height="100" Margin="1,165,-2,0" VerticalAlignment="Top" Width="293"/>

    </Grid>
</Window>
